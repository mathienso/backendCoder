<div>
    <h1>Lista de productos:</h1>
    <br><br>
    {{# each products}}
    <p>Titulo: {{title}}</p>
    <p>Descripcion: {{description}}</p>
    <p>Precio: {{price}}</p>
    <p>Status: {{status}}</p>
    <p>Thumbnail: {{thumbnail}}</p>
    <p>Code: {{code}}</p>
    <p>Stock: {{stock}}</p>
    <p>Id: {{id}}</p>
    <br><br>
    {{/each}}
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>